{
   "content_scripts": [ {
      "js": [ "main.js" ],
      "matches": [ "https://github.com/*"]
   } ],
   "description": "Adds useful buttons on repos' homepage to clone in VSCode or open in GitHub1s",
   "manifest_version": 2,
   "name": "GitHub - VSCode",
   "short_name": "GHVSCode",
   "version": "0.0.1",
   "web_accessible_resources": [
      "img/*"
   ],
   "icons": {
      "128": "img/logo.png",
      "16": "img/logo.png",
      "48": "img/logo.png"
   }
}
